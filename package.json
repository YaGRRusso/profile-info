{
  "name": "personal-info-api",
  "version": "0.0.1",
  "description": "",
  "author": "YaGRRusso",
  "private": true,
  "license": "MIT",
  "main": "dist/src/",
  "engines": {
    "node": "18.x",
    "npm": "10.x"
  },
  "scripts": {
    "build": "npx nest build",
    "sdk": "openapi-generator-cli generate",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "npm run docker:up & npm run prisma:studio & npm run start:dev",
    "start": "node dist/src/main",
    "start:dev": "nest start --watch",
    "start:repl": "nest start --entryFile repl",
    "start:debug": "nest start --watch --debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.config.ts",
    "prepare": "husky install",
    "prisma:dev": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset",
    "prisma:deploy": "npx prisma migrate deploy",
    "prisma:studio": "npx prisma studio",
    "docker:up": "docker compose up",
    "docker:down": "docker compose down",
    "docker:ls": "docker container ls"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.9",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/swagger": "^7.3.1",
    "@prisma/client": "^5.15.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@yagrrusso/eslint-config": "^1.3.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.6",
    "prettier": "^3.3.2",
    "prisma": "^5.15.0",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "**/*.{js, jsx, ts, tsx}": [
      "eslint --fix"
    ]
  }
}